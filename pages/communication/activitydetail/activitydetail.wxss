/*展开收起*/
.fold_extend1 {
    content: '展开';
    font-size: 32rpx;
    float: right;
    padding-right: 20rpx;
}
.fold_extend2 {
    content: '收起';
    font-size: 28rpx;
    float: right;
    color: #45AB59;
    padding-right: 20rpx;
}
.fold_extend_textextend {
    overflow: visible;
    display: -webkit-box;
    -webkit-box-orient: vertical;
}
.fold_extend_textfold {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
}
/*立即注册*/
.register_view {
    display: inline-block;
    position: fixed;
    bottom: 0;
    background-color: white;
    width: 100%;
    height: 90rpx;
}
.tagview {
    color: #DF3C43;
    font-size: 24rpx;
    float: left;
    text-align: center;
    width: 60%;
    height: 90rpx;
    line-height: 90rpx;
    border-top: 1rpx solid #EFEFEF;
}
.registernow_btn {
    float: right;
    font-size: 36rpx;
    width: 40%;
    height: 90rpx;
    line-height: 90rpx;
    z-index: 100;
    vertical-align: center;
    text-align:center;
    color: #fff;
    background-color: #DF3C43;
}
.disabled_registernow_btn {
    float: right;
